<?xml version="1.0" encoding="UTF-8"?>

<?import de.jensd.fx.glyphs.fontawesome.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane maxWidth="Infinity" minWidth="-Infinity" prefWidth="600.0" styleClass="pane_container" stylesheets="@../../Styles/BeneficiariesCell.css"
            xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1">
   <HBox prefHeight="58.0" styleClass="pane_container" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <StackPane prefHeight="100.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
         <Label fx:id="policyOwnerIdLbl" alignment="CENTER" prefHeight="18.0" prefWidth="91.0" text="c0000001" textAlignment="CENTER">
            <font>
               <Font name="System Bold" size="12.0" />
            </font>
         </Label>
      </StackPane>
      <StackPane layoutX="10.0" layoutY="10.0" prefHeight="100.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
         <Label fx:id="policyOwnerNameLbl" alignment="CENTER" prefHeight="18.0" prefWidth="131.0" text="Hoang">
            <font>
               <Font name="System Bold" size="12.0" />
            </font>
         </Label>
      </StackPane>
      <StackPane layoutX="213.0" layoutY="10.0" prefHeight="100.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
         <Label fx:id="policyOwnerFeeLbl" alignment="CENTER" prefHeight="18.0" prefWidth="123.0" text="0123456789">
            <font>
               <Font name="System Bold" size="12.0" />
            </font>
         </Label>
      </StackPane>
      <StackPane alignment="CENTER_RIGHT" layoutX="596.0" layoutY="10.0" prefHeight="100.0" prefWidth="300.0" HBox.hgrow="ALWAYS">
         <HBox alignment="CENTER_RIGHT" spacing="10.0">
            <StackPane.margin>
               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </StackPane.margin>
            <Button fx:id="showPolicyHolderList" mnemonicParsing="false">
               <tooltip>
                  <Tooltip text="Show policy holder list" />
               </tooltip>
               <graphic>
                  <FontAwesomeIconView glyphName="LIST" size="20" />
               </graphic>
            </Button>
            <Button fx:id="updateInfoBtn" mnemonicParsing="false">
               <tooltip>
                  <Tooltip text="Update information" />
               </tooltip>
               <graphic>
                  <FontAwesomeIconView glyphName="EDIT" size="20" />
               </graphic>
            </Button>
            <Button fx:id="deleteBtn" mnemonicParsing="false">
               <tooltip>
                  <Tooltip text="Delete policy holder" />
               </tooltip>
               <graphic>
                  <FontAwesomeIconView glyphName="TRASH" size="20" />
               </graphic>
            </Button>
         </HBox>
      </StackPane>
   </HBox>
</AnchorPane>
