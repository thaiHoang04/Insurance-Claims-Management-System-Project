<?xml version="1.0" encoding="UTF-8"?>

<?import de.jensd.fx.glyphs.fontawesome.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>

<AnchorPane styleClass="pane_container" stylesheets="@../../Styles/BeneficiariesCell.css"
            xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1">
   <HBox prefHeight="34.0" prefWidth="782.0" styleClass="pane_container">
      <StackPane prefHeight="100.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
         <Label fx:id="insuranceManagerIdLbl" alignment="CENTER" prefHeight="18.0" prefWidth="549.0" text="c0000001" textAlignment="CENTER">
            <font>
               <Font name="System Bold" size="12.0" />
            </font>
         </Label>
      </StackPane>
      <Line blendMode="OVERLAY" endY="50.0" strokeWidth="3.0">
         <HBox.margin>
            <Insets top="5.0" />
         </HBox.margin>
      </Line>
      <StackPane layoutX="10.0" layoutY="10.0" prefHeight="100.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
         <Label fx:id="insuranceManagerNameLbl" alignment="CENTER" prefHeight="18.0" prefWidth="635.0" text="Hoang">
            <font>
               <Font name="System Bold" size="12.0" />
            </font>
         </Label>
      </StackPane>
      <Line blendMode="OVERLAY" endY="50.0" layoutX="212.0" layoutY="16.0" strokeWidth="3.0">
         <HBox.margin>
            <Insets top="5.0" />
         </HBox.margin>
      </Line>
      <StackPane layoutX="213.0" layoutY="10.0" prefHeight="100.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
         <Label fx:id="insuranceManagerEmailLbl" alignment="CENTER" prefHeight="18.0" prefWidth="603.0" text="0123456789">
            <font>
               <Font name="System Bold" size="12.0" />
            </font>
         </Label>
      </StackPane>
      <Line blendMode="OVERLAY" endY="50.0" layoutX="415.0" layoutY="16.0" strokeWidth="3.0" />
      <StackPane layoutX="416.0" layoutY="10.0" prefHeight="100.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
         <Label fx:id="insuranceManagerPhoneNumLbl" alignment="CENTER" prefHeight="18.0" prefWidth="566.0" text="0903789567">
            <font>
               <Font name="System Bold" size="12.0" />
            </font>
         </Label>
      </StackPane>
      <Line blendMode="OVERLAY" endY="50.0" layoutX="597.0" layoutY="12.0" strokeWidth="3.0" />
      <StackPane layoutX="596.0" layoutY="10.0" prefHeight="100.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
         <HBox alignment="CENTER_RIGHT" prefHeight="38.0" prefWidth="97.0" spacing="10.0">
            <StackPane.margin>
               <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </StackPane.margin>
            <Button fx:id="updateInfoBtn" mnemonicParsing="false">
               <tooltip>
                  <Tooltip text="Update information" />
               </tooltip>
               <graphic>
                  <FontAwesomeIconView glyphName="EDIT" size="20" />
               </graphic>
            </Button>
            <Button fx:id="deleteBtn" mnemonicParsing="false">
               <tooltip>
                  <Tooltip text="Delete insurance manager" />
               </tooltip>
               <graphic>
                  <FontAwesomeIconView glyphName="TRASH" size="20" />
               </graphic>
            </Button>
         </HBox>
      </StackPane>
   </HBox>
</AnchorPane>
